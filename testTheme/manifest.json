{

    "description": "Theme using a PNG as the theme_frame image, employing a faded edge. See https://developer.mozilla.org/en-US/Add-ons/WebExtensions/Examples#themes",
    "manifest_version": 2,
    "name": "test",
    "version": "1.1",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/themes/weta_fade",
  
    "theme": {
      "images": {
        "theme_frame": "test.png"//, // Chrome = Anchors top left
  
        //"additional_backgrounds": []
      },
      
      "colors": {// Chrome = "colors": [0, 0, 0]
         "frame": "black",
         "tab_text": "white",
  
         //"accentcolor: "",
         "bookmark_text": "white",
         "button_background_active": "black",
         "button_background_hover": "black",
         "icons": "white",
         "icons_attention": "black",
         
         "frame_inactive": "black",
         "ntp_background": "black",
         "ntp_text": "white",
         "popup": "black",
         "popup_border": "black",
         "popup_highlight": "black",
         "popup_highlight_text": "white",
         "popup_text": "white",
         "sidebar": "black",
         "sidebar_border": "black",
         "sidebar_highlight": "black",
         "sidebar_highlight_text": "white",
         "sidebar_text": "white",
         //"tab_background_separator": "",
         "tab_background_text": "white",
         "tab_line": "black",
         "tab_loading": "black",
         "tab_selected": "black",
         
         //"textcolor": "",
         "toolbar": "black",
